    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "6.0.2",
    "recharts": "^2.15.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.2",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "postcss": "8.4.8",
    "prettier": "^3.5.3",
    "tailwindcss": "3.4.6",
    "typescript": "~5.7.2",
    "vite": "5.0.0"
